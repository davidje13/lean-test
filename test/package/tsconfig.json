{
	"compilerOptions": {
		"target": "ES6",
		"moduleResolution": "node",
		"strict": true,
		"exactOptionalPropertyTypes": true,
		"noImplicitOverride": true,
		"noImplicitReturns": true,
		"noPropertyAccessFromIndexSignature": true,
		"noUnusedLocals": true,
		"importsNotUsedAsValues": "error",
		"noEmitOnError": true
	},
	"include": [
		"."
	]
}
